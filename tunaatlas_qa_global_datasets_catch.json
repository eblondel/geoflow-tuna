{
  "profile": {
    "id": "tuna-atlas-catch",
    "mode": "entity",
  	"project": "FIRMS Tuna Atlas",
  	"organization": "FIRMS Tuna Atlas",
  	"logos": [
  		"http://firms.fao.org/figis/website/assets/images/templates/firms/firms_banner.gif",
  		"https://www.blue-cloud.org/sites/default/files/styles/logo_small/public/FAO_2lines_500x500_1.png?itok=igwPSL5v",
  		"https://en.ird.fr/sites/ird_fr/files/2019-08/logo_IRD_2016_BLOC_UK_COUL.png"
  	],
  	"options": {
  	  "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities" : [
      {
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/1JhS1ADsnqTpWOEEvljk-c9EdzBurkc8A8__JOHJmkOk/edit#gid=163629835"
      }
    ],
    "contacts" : [
      {
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/1DtGR8cHAkro12DOFZn40cehfU9LoI9l7wVUCHfHH-xY/edit?usp=sharing"
      }
    ],
    "dictionary" : [
      {
  	  "handler": "gsheet",
  	  "source": "https://docs.google.com/spreadsheets/d/166f_fIB9ctwdsRI8MCqKmfYtyh6DPbipHp8_qVjrIs8/edit#gid=0"
  	  }
    ]
  },
  "software": [
	{
		"id": "my-database-read",
		"type": "input",
		"software_type": "dbi",
		"parameters": {
			"drv": "PostgreSQL",
			"user": "{{DB_USER}}",
			"password": "{{DB_PASSWORD}}",
			"host": "{{DB_HOST}}",
			"port": "{{DB_PORT}}",
			"dbname": "{{DB_NAME}}"
		},
		"properties" : {}
	},
	{
		"id": "my-database-write",
		"type": "output",
		"software_type": "dbi",
		"parameters": {
			"drv": "PostgreSQL",
			"user": "{{DB_USER}}",
			"password": "{{DB_PASSWORD}}",
			"host": "{{DB_HOST}}",
			"port": "{{DB_PORT}}",
			"dbname": "{{DB_NAME}}"
		},
		"properties" : {}
	},
		{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
			"email": "{{GMAIL_USER}}",
			"token": "{{GMAIL_TOKEN}}"
		},
		"properties" : {}
	}
  ],
  "actions": [
  
	
	{
		"id": "load_dataset",
		"script": "https://raw.githubusercontent.com/firms-gta/geoflow-tunaatlas/master/tunaatlas_actions/load_dataset.R",
		"run": true,
		"options": {
			"upload_to_db": true,
			"create_materialized_view" : true,
			"add_sql_comments" : true,
			"upload_to_googledrive": true
		}
	},
	{
		"id": "enrich_metadata",
		"script": "https://raw.githubusercontent.com/eblondel/geoflow-tunaatlas/master/tunaatlas_actions/enrich_metadata.R",
		"run": true
	},
	{
		"id": "enrich_for_db_services",
		"script": "https://raw.githubusercontent.com/eblondel/geoflow-tunaatlas/master/tunaatlas_actions/enrich_db_for_services.R",
		"run": true
	},
	{
		"id": "load_metadata",
		"script": "https://raw.githubusercontent.com/eblondel/geoflow-tunaatlas/master/tunaatlas_actions/load_metadata.R",
		"run": true
	},
	{		"id": "comparison_each_step",
		"script": "https://raw.githubusercontent.com/BastienIRD/Tunaatlas_level1/main/recap_all_markdown.R",
		"run": false
	}
	

 ]
}
